{
  "name": "01-jest",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "tdd": "onchange 'src/**/*.js' -- npm run test",
    "coverage": "jest --collectCoverage"
  },
  "devDependencies": {
    "axios": "^0.15.3",
    "babel-jest": "^17.0.2",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "chance": "^1.0.4",
    "jest": "^17.0.3",
    "jest-cli": "^16.0.1",
    "onchange": "^3.0.2"
  },
  "jest": {
    "collectCoverage": false,
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "globals": {
      "__DEV__": true
    }
  },
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "jest": "^19.0.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  }
}
